{
    "url": "/scenes",
    "method": "GET",
    "response": {
        "output": "{{parseTimestamps(item)}}",
        "iterate": "{{body.items}}",
        "limit": "{{parameters.limit}}"
    },
    "qs": {
        "locationId": "{{parameters.locationId}}",
        "max": 100
    },
    "pagination": {
        "condition": "{{ifempty(body._links.next.href, false)}}",
        "url": "{{body._links.next.href}}"
    }
}