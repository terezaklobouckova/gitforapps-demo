{
    "url": "/locations/{{parameters.id}}",
    "method": "GET",
    "qs": {
        "allowed": "{{parameters.allowed}}"
    },
    "body": {
        "name": "{{parameters.name}}",
        "email": "{{lower(parameters.email)}}"
    },
    "response": {
        "output": "{{parseTimestamps(body)}}"
    }
}